//progam 1

import java.util.Random;
import java.util.Scanner;

public class lecture3 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in); 

        Random rnd= new Random();

        int computerChoice = rnd.nextInt(20);

        System.out.println("Computer Choice : "+computerChoice);



        System.out.println("Guess a Number between 0 and 20 ");

        int userChoice = sc.nextInt();

        if (userChoice>computerChoice){

            System.out.println("Your Guess is Too High");}

else if (userChoice< computerChoice) { System.out.println("Your Guess is Too Low");

            }
else {

            System.out.println("Gotcha! You Guessed it Right!!");

        }

    }



    }


//program 2


import java.util.Random;
import java.util.Scanner;

public class lecture3 {
    public static void main(String[] args) {


            char ch='A';

            while(ch<=90) {

                System.out.print(ch + " ");
                ch++; // 65 + 1

            }

                char chl='A';
            do {

                    System.out.print ( ch + " ");

                    ch++;

                }

                while(ch<='Z');
    }
}

//program 3

import java.util.Random;
import java.util.Scanner;

public class lecture3 {
    public static void main(String[] args) {

                Scanner sc = new Scanner(System.in);

                System.out.println("Enter Your Division ");

                int div = sc.nextInt();

                switch(div) {

                    case 1:
                        System.out.println("First ");
                        break;
                    case 2:
                        System.out.println("Second ");
                        break;
                    case 3:
                        System.out.println("Third");
                        break;
                        


                }

            }
    }

//program 4

import java.util.Random;
import java.util.Scanner;

public class lecture3 {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);

        System.out.println("How Long is Your Burp ?");

        int burpLength = sc.nextInt();

        String msg="";

        for(int i=1 ; i <= burpLength; i++) {

            msg =msg+"r"; // can also be written as msg +=^ prime prime r^ prime prime

        }

        System.out.println("Bu"+msg+"p");
            }
    }


